CFLAGS= -Wall -Wextra -Wfatal-errors -O3 -fopenmp
VECFLAGS= -fopt-info-vec-all -ftree-vectorizer-verbose=0

all: add add_v add_parallel
add: add.c
	icc $< -o $@ $(CFLAGS)
add_v: add_v.c
	icc -O3 $< -o $@ $(CFLAGS)
add_parallel: add_parallel.c
	icc -O3 $< -o $@ $(CFLAGS)
clean:
	rm -rf *.o add add_v add_parallel

